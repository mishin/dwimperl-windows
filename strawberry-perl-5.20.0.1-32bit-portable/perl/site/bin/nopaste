#!/usr/bin/env perl
use strict;
use warnings;
# PODNAME: command-line utility to nopaste

use App::Nopaste::Command;

if (my $url = App::Nopaste::Command->new_with_options->run) {
    print $url;
    print "\n";
}

__END__

